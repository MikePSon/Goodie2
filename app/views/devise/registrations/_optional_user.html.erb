
<div class="form-group">
  <%= f.label :job_title, :class => "col-lg-2 control-label" %>
  <div class="col-lg-10">
    <%= f.text_field :job_title, class: "form-control", :placeholder => "Job Title" %>
  </div>
</div>

<div class="form-group">
  <%= f.label :phone, :class => "col-lg-2 control-label" %>
  <div class="col-lg-10">
    <%= f.text_field :phone, :class => "form-control", :placeholder => "(###)###-####" %>
  </div>
</div>

<div class="form-group">
  <%= f.label :office_open, :class => "col-lg-2 control-label" %>
  <div class="col-lg-10">
    <div id="openTimePicker" class="input-group date">
      <%= f.text_field :office_open, as: :default, :class => "form-control m-b", :placeholder => "HH:MM AM/PM" %>
      <span class="input-group-addon">
        <span class="fa fa-calendar"></span>
      </span>
    </div>
  </div>
</div>

<div class="form-group">
  <%= f.label :office_close, :class => "col-lg-2 control-label" %>
  <div class="col-lg-10">
    <div id="closeTimePicker" class="input-group date">
      <%= f.text_field :office_close, as: :default, :class => "form-control m-b", :placeholder => "HH:MM AM/PM" %>
      <span class="input-group-addon">
        <span class="fa fa-calendar"></span>
      </span>
    </div>
  </div>
</div>