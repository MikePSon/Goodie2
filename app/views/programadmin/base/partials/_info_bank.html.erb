<div class="panel widget">
   <div class="row row-table row-flush">
      <div class="col-xs-6 bb br">
         <div class="row row-table row-flush">
            <div class="col-xs-4 text-center text-info">
               <em class="fa fa-users fa-2x"></em>
            </div>
            <div class="col-xs-8">
               <div class="panel-body text-center">
                  <h4 class="mt0"><%= @all_applicants.count %></h4>
                  <p class="mb0 text-muted">APPLICANTS</p>
               </div>
            </div>
         </div>
      </div>
      <div class="col-xs-6 bb">
         <div class="row row-table row-flush">
            <div class="col-xs-4 text-center text-danger">
               <em class="fa fa-dollar fa-2x"></em>
            </div>
            <div class="col-xs-8">
               <div class="panel-body text-center">
                  <h4 class="mt0">$<%= @amount_requested %></h4>
                  <p class="mb0 text-muted">REQUESTED</p>
               </div>
            </div>
         </div>
      </div>
   </div>
   <div class="row row-table row-flush">
      <div class="col-xs-6 br">
         <div class="row row-table row-flush">
            <div class="col-xs-4 text-center text-inverse">
               <em class="fa fa-file-text-o fa-2x"></em>
            </div>
            <div class="col-xs-8">
               <div class="panel-body text-center">
                  <h4 class="mt0"><%= @my_projects.count %></h4>
                  <p class="mb0 text-muted">PROJECTS</p>
               </div>
            </div>
         </div>
      </div>
      <div class="col-xs-6">
         <div class="row row-table row-flush">
            <div class="col-xs-4 text-center text-success">
               <em class="icon-refresh fa-2x"></em>
            </div>
            <div class="col-xs-8">
               <div class="panel-body text-center">
                  <h4 class="mt0"><%= @my_cycles.count %></h4>
                  <p class="mb0 text-muted">CYCLES</p>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>
<!-- END widget-->
